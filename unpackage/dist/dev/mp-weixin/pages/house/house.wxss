/* 水平间距 */
/* 水平间距 */
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
swiper {
  height: 330rpx;
}
swiper .swiper-item, swiper image {
  height: 100%;
  width: 100%;
}
.nav-list {
  display: flex;
  justify-content: space-around;
  margin: 15px 0;
}
.nav-list image {
  width: 128rpx;
  height: 140rpx;
}
.floor-title {
  width: 100%;
  height: 60rpx;
}
.product {
  display: flex;
}
.product .product-left {
  padding-left: 10rpx;
}
.product .product-right {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}

